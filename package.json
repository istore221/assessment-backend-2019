{
  "name": "assignment-backend-2019",
  "version": "0.0.1",
  "main": "src/server.js",
  "scripts": {
    "prestart": "docker-compose up -d mongo",
    "babel": "copyfiles .dockerignore Dockerfile package.json pm2.cluster.config.js pm2.dev.config.js test/**/* config/**/* dist && babel src --out-dir dist --copy-files",
    "start": "cross-env NODE_ENV=development npm stop && npm run lint && pm2-dev pm2.dev.config.js",
    "stop": "pm2 delete pm2.dev.config.js pm2.cluster.config.js",
    "lint": "eslint src --ext .js",
    "build": "npm run lint && npm run babel",
    "release": "cross-env NODE_ENV=production && npm run build",
    "test": "npm run prestart && jest --silent"
  },
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.5.0",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "graphql": "^14.3.1",
    "graphql-tools": "^4.0.5",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.6.0",
    "mongoose-paginate-v2": "^1.3.0",
    "nodemon": "^1.19.1",
    "qs-middleware": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "copyfiles": "^2.1.0",
    "core-js": "^3.1.4",
    "eslint": "^6.0.0",
    "jest": "^24.8.0",
    "supertest": "^4.0.2"
  }
}
